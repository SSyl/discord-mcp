# Required: Your Discord account credentials
DISCORD_EMAIL=your_email@example.com
DISCORD_PASSWORD=your_password

# Optional: Browser settings
DISCORD_HEADLESS=true

# Optional: Server and message limits
DISCORD_GUILD_IDS=guild_id1,guild_id2,guild_id3
MAX_MESSAGES_PER_CHANNEL=200
DEFAULT_HOURS_BACK=24

# Test configuration (for running integration tests)
TEST_SERVER_ID=your_test_server_id
TEST_CHANNEL_ID=your_test_channel_id
TEST_SEARCH_QUERY=test
TEST_SEARCH_CHANNEL=general